@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@MPBasicSetting.WeChatTitleName</title>
    <link href="~/Content/weui.css" rel="stylesheet" />
    <link href="~/Content/jquery-weui.css" rel="stylesheet" />
    <script src="~/Scripts/fastclick.js"></script>
    <script src="~/Scripts/jquery-2.1.4.js"></script>
    <script src="~/Scripts/jquery-weui.min.js"></script>
    <style>
        body, html {
            height: 100%;
            -webkit-tap-highlight-color: transparent;
        }

        .placeholder {
            margin: 7px;
        }

            .placeholder a {
                height: 70px;
                line-height: 70px;
                font-size: 20px;
            }

            .page-header {
            height: 175px;
            background-color: #01A9DA;
            box-shadow: 5px 5px 10px rgba(0, 0, 0,0.3);
            z-index: -99;
        }

        .headImage {
            margin-top: 20px;
            margin-left: 20px;
            width: 100px;
            height: 100px;
        }

        .big-circle {
            position: absolute;
            width: 100px;
            height: 100px;
            background-color: #6CC8E2;
            border-radius: 50px;
        }

        .small-circle {
            position: absolute;
            width: 90px;
            height: 90px;
            background: url("@ViewBag.HeadImageUrl") no-repeat center center;
            background-size: 100% 100%;
            border-radius: 45px;
            margin-left: 5px;
            margin-top: 5px;
        }

        .wxname {
            color: white;
            font-weight: bold;
            font-size: 25px;
            padding-top: 50px;
            padding-left: 10px;
        }

        .company {
            color: white;
        }


        .weui-grid__icon {
            width: 40px;
            height: 40px;
        }

        .weui-footer {
            margin-top: 80px;
        }

        .mail {
            float: right;
            margin-right: 10px;
            margin-top: 10px;
            width: 28px;
            height: 28px;
            background: url(/Images/mail.png) no-repeat center center;
            background-size: 100% 100%;
        }

        .swiper-container {
            height: 100px;
        }

        .swiper-container {
            box-shadow: 5px 5px 10px rgba(0, 0, 0,0.3);
        }

        .swiper-slide img {
            background-repeat: no-repeat;
            background-position-x: center;
            background-position-y: center;
            background-size: 100% 100%;
        }
    </style>
</head>
<body>
    <body ontouchstart>

        <div class="weui-tab">
            <div class="weui-tab__bd">
                <div id="tab1" class="weui-tab__bd-item weui-tab__bd-item--active">
                    <img src="~/images/show.png" width="100%" height="150px" />
                    <div class="weui-flex" style="margin:5px">
                        <div class="weui-flex__item ">
                            <div class="placeholder">
                                <a id="btnYes" href="javascript:void(0);" class="weui-btn weui-btn_warn">我要推荐</a>
                            </div>
                        </div>
                        <div class="weui-flex__item">
                            <div class="placeholder">
                                <a id="btnNo" href="javascript:void(0);" class="weui-btn weui-btn_warn">
                                    我的客户
                                </a>
                            </div>
                        </div>
                    </div>
                    <div class="weui-flex" style="margin: 5px">
                        <div class="weui-flex__item ">
                            <div class="placeholder">
                                <a id="btnYes" href="javascript:void(0);" class="weui-btn weui-btn_warn">我的佣金</a>
                            </div>
                        </div>
                        <div class="weui-flex__item">
                            <div class="placeholder">
                                <a id="btnNo" href="javascript:void(0);" class="weui-btn weui-btn_warn">活动规则</a>
                            </div>
                        </div>
                    </div>
                    <div class="weui-panel weui-panel_access" style="margin-bottom:70px">
                        <div class="weui-panel__bd">
                            <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                                <div class="weui-media-box__hd">
                                    <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                                </div>
                                <div class="weui-media-box__bd">
                                    <h4 class="weui-media-box__title">中学语文</h4>
                                    <p class="weui-media-box__desc">由知名教师授课 1对1教学</p>
                                </div>
                            </a>
                            <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                                <div class="weui-media-box__hd">
                                    <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                                </div>
                                <div class="weui-media-box__bd">
                                    <h4 class="weui-media-box__title">小数数学</h4>
                                    <p class="weui-media-box__desc">由知名教师授课 1对1教学</p>
                                </div>
                            </a>
                            <a href="javascript:void(0);" class="weui-media-box weui-media-box_appmsg">
                                <div class="weui-media-box__hd">
                                    <img class="weui-media-box__thumb" src="data:image/png;base64," alt="">
                                </div>
                                <div class="weui-media-box__bd">
                                    <h4 class="weui-media-box__title">g高中物理</h4>
                                    <p class="weui-media-box__desc">由知名教师授课 1对1教学</p>
                                </div>
                            </a>
                        </div>


                    </div>
                </div>
                <div id="tab2" class="weui-tab__bd-item">
                </div>
                <div id="tab3" class="weui-tab__bd-item">
                    <iframe src=""></iframe>
                </div>
                <div id="tab4" class="weui-tab__bd-item">
                    <div class="page-header">
                        <div class="weui-flex">
                            <div class="weui-flex__item">
                                <div class="headImage">
                                    <div class="big-circle"></div>
                                    <div class="small-circle"></div>
                                </div>
                            </div>
                            <div class="weui-flex__item">
                                <div class="wxname">@ViewBag.UserName</div>

                            </div>
                            <div class="weui-flex__item">
                                <div class="mail"></div>
                            </div>
                        </div>
                        <div class="weui-cells__title" style="color:white;text-align:right;font-size:16px">@ViewBag.CompanyName</div>
                    </div>
                    <div class="weui-cells">
                        <a class="weui-cell weui-cell_access" href="javascript:;">
                            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
                            <div class="weui-cell__bd">
                                <p>个人设置</p>
                            </div>
                            <div class="weui-cell__ft">个人设置</div>
                        </a>
                        <a class="weui-cell weui-cell_access" href="javascript:;">
                            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
                            <div class="weui-cell__bd">
                                <p>我的佣金</p>
                            </div>
                            <div class="weui-cell__ft">我的佣金</div>
                        </a>
                        <a class="weui-cell weui-cell_access" href="javascript:;">
                            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
                            <div class="weui-cell__bd">
                                <p>我的邀请</p>
                            </div>
                            <div class="weui-cell__ft">我的邀请</div>
                        </a>
                        <a class="weui-cell weui-cell_access" href="javascript:;">
                            <div class="weui-cell__hd"><img src="data:image/png;base64," alt="" style="width:20px;margin-right:5px;display:block"></div>
                            <div class="weui-cell__bd">
                                <p>我的客户</p>
                            </div>
                            <div class="weui-cell__ft">我的客户</div>
                        </a>
                    </div>
                </div>
            </div>

            <div class="weui-tabbar">
                <a href="#tab1" class="weui-tabbar__item weui-bar__item--on">
                    @*<span class="weui-badge" style="position: absolute;top: -.4em;right: 1em;">8</span>*@
                    <div class="weui-tabbar__icon">
                        <img src="~/images/icon_nav_button.png" alt="">
                    </div>
                    <p class="weui-tabbar__label">首页</p>
                </a>
                <a href="#tab2" class="weui-tabbar__item">
                    <div class="weui-tabbar__icon">
                        <img src="~/images/icon_nav_msg.png" alt="">
                    </div>
                    <p class="weui-tabbar__label">客户</p>
                </a>
                <a href="#tab3" class="weui-tabbar__item">
                    <div class="weui-tabbar__icon">
                        <img src="~/images/icon_nav_article.png" alt="">
                    </div>
                    <p class="weui-tabbar__label">动态</p>
                </a>
                <a href="#tab4" class="weui-tabbar__item">
                    <div class="weui-tabbar__icon">
                        <img src="~/images/icon_nav_cell.png" alt="">
                    </div>
                    <p class="weui-tabbar__label">我</p>
                </a>
            </div>
        </div>
        <script>
            $(function () {
                FastClick.attach(document.body);
            });
        </script>
        <script>
            $("#a").select({
                title: "选择职业",
                items: ["法官", "医生", "猎人", "学生", "记者", "其他"]
            });
        </script>
    </body>
</html>



